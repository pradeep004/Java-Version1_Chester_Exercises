
public class Fruit_shop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String fruit = "mango";
		
		switch (fruit) {
		case "Apple":
			System.out.println("The price of apple is 20$");
			break;
		case "Grapes":
			System.out.println("The price of Grapes is 3$");
			break;
		case "banana":
			System.out.println("The price of banana is 20$");
			break;
		case "orange":
			System.out.println("The price of orange is 20$");
			break;
		default:
			System.out.println("THe price of mango is 21$");
			break;
		}
	}

}
