
public class Operators {
	int first_no = 20;
	int second_no = 10;
	public int addition(int first_no, int second_no) {
		System.out.println("The addtion of" + first_no + second_no +"=" + (first_no+second_no) );
		return (first_no+second_no);
	}
	

	public int substraction(int first_no, int second_no) {
		System.out.println("The substraction of" + first_no +"+" +second_no +"=" + (first_no+second_no) );
		return (first_no-second_no);
	}
	
	public int multiplication(int first_no, int second_no) {
		System.out.println("The multiplication of" + first_no +"-" +second_no +"=" + (first_no*second_no) );
		return (first_no*second_no);
	}
	
	public int devision(int first_no, int second_no) {
		System.out.println("The devision of" + first_no + second_no +"=" + (first_no/second_no) );
		return (first_no/second_no);
	}
	
}
