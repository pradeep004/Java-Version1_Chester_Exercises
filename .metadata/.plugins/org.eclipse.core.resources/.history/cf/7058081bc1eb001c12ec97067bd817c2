
public class Charecter {
	String name ;
	String clothing;
	String background;
	int speed ;
	String Hair ;
	Charecter(String name, String clothing, String Hair){
		this.name = name;
		this.clothing= clothing;
		this.Hair= Hair;
		
	}
	Charecter (String name, int speed, String background){
		this.name=name;
		this.speed=speed;
		this.background =background;
	}
	Charecter(){	
	}
	
	

}
